{
    "manifest_version": 2,
  
    "name": "LatiTools Extension",
    "description": "This extension produces a bounded box around inputted coordinates.",
    "version": "1.0",
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  
    "browser_action": {
     "default_icon": "ic_main.png",
     "default_popup": "popup.html"
    },
    "permissions": [
     "activeTab"
     ]
  }
  